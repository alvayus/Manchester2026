{
  "extends": ["tslint:latest", "tslint-config-prettier", "tslint-immutable"],
  "defaultSeverity": "error",
  "jsRules": {},
  "rules": {
    "no-bitwise": false,
    "no-implicit-dependencies": [true, ["dev", "frontpanel-ws"]],
    "no-submodule-imports": [true, "bootstrap/dist/css/bootstrap.css"],
    "object-literal-sort-keys": false,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    }
  },
  "rulesDirectory": []
}