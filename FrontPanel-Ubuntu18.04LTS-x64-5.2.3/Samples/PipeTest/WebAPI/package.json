{
  "name": "frontpanel-sample-pipetest",
  "version": "0.1.0",
  "description": "FrontPanel Web API Pipetest sample",
  "main": "lib/common.js",
  "scripts": {
    "tsc": "tsc",
    "start": "run-s clean tsc start:*",
    "start:parcel": "parcel serve ./src/index.html",
    "build": "run-s clean tsc build:*",
    "build:parcel": "parcel build ./src/index.html --public-url .",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "clean": "trash .cache dist lib"
  },
  "author": "",
  "license": "",
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "@types/node": "^12.12.22",
    "bootstrap": "^4.4.1",
    "@opalkelly/frontpanel-samples-common": "file:../../WebAPI",
    "jquery": "^3.4.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "posthtml-beautify": "^0.3.1",
    "posthtml-expressions": "^1.1.1",
    "posthtml-include": "^1.3.2",
    "prettier": "^1.19.1",
    "trash-cli": "^3.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "typescript": "^3.7.4"
  }
}
